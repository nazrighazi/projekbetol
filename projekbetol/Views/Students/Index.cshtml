@model IEnumerable<projekbetol.Student>

@{
    ViewBag.Title = "Student Information";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<div>
    <iframe  width="1000" height="500" src="https://docs.google.com/forms/d/e/1FAIpQLScnbJ9vqdp5UTlV7E3Ccwv-847V5V0Rhg6wFHbi1bH4r2z_DQ/viewform?usp=sf_link"></iframe>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentCode)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentCode)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>

        </tr>
    }
        
</table>

